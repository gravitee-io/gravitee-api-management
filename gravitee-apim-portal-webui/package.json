{
  "name": "@gravitee/portal-webui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Gravitee.io Portal",
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=8192 ng build",
    "build:prod": "NODE_OPTIONS=--max_old_space_size=8192 ng build --configuration production",
    "lint": "ng lint && yarn prettier",
    "lint:fix": "ng lint --fix && yarn prettier:fix && yarn lint:license:fix",
    "lint:license": "license-check-and-add check -f license-check-config.json",
    "lint:license:fix": "license-check-and-add add -f license-check-config.json -r",
    "ng": "ng",
    "prettier": "prettier --check \"**/*.{js,ts,html,css,scss,json}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,html,css,scss,json}\"",
    "prettier:list": "prettier --list-different \"**/*.{js,ts,html,css,scss,json}\"",
    "serve": "NODE_OPTIONS=--max_old_space_size=8192 ng serve --port=4100",
    "serve:apim-master": "NODE_OPTIONS=--max_old_space_size=8192 BACKEND_ENV=apim-master-api.team-apim.gravitee.dev ng serve --port=4100",
    "serve:prod": "node scripts/serve.js",
    "test": "NODE_OPTIONS=--max-old-space-size=6144 ng test",
    "test:coverage": "JEST_JUNIT_ADD_FILE_ATTRIBUTE=true JEST_JUNIT_OUTPUT_DIR=\"coverage\" NODE_OPTIONS=--max-old-space-size=6144 ng test --ci --reporters=default --reporters=jest-junit --max-workers=3 --collect-coverage",
    "test:watch": "NODE_OPTIONS=--max-old-space-size=6144 ng test --watch -w 8",
    "update:sdk": "sh ./scripts/update-sdk.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.16",
    "@angular/common": "20.3.16",
    "@angular/compiler": "20.3.16",
    "@angular/core": "20.3.16",
    "@angular/forms": "20.3.16",
    "@angular/localize": "20.3.16",
    "@angular/platform-browser": "20.3.16",
    "@angular/platform-browser-dynamic": "20.3.16",
    "@angular/router": "20.3.16",
    "@asciidoctor/core": "3.0.4",
    "@asyncapi/web-component": "2.6.3",
    "@emoji-mart/data": "1.2.1",
    "@fontsource/dm-mono": "5.2.5",
    "@fontsource/dm-sans": "5.2.5",
    "@fontsource/ibm-plex-sans": "5.2.5",
    "@fontsource/montserrat": "5.2.5",
    "@gravitee/ui-components": "4.3.2",
    "@highcharts/map-collection": "2.3.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@types/swagger-ui": "3.52.4",
    "angular-oauth2-oidc": "13.0.1",
    "asciidoctor": "3.0.4",
    "asciidoctor-highlight.js": "0.4.0",
    "clipboard-copy": "4.0.1",
    "github-slugger": "2.0.0",
