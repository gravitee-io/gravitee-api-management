{
  "id": "api-streaming-greeter-v4",
  "name": "streaming-greeter-v4",
  "gravitee": "4.0.0",
  "type": "sync",
  "listeners": [
    {
      "type": "http",
      "paths": [
        {
          "path": "/manualflowcontrol.StreamingGreeter"
        }
      ],
      "entrypoints": [
        {
          "type": "http-proxy"
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default-group",
      "type": "http-proxy",
      "endpoints": [
        {
          "name": "default",
          "type": "http-proxy",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "target": "grpc://localhost:50051/manualflowcontrol.StreamingGreeter"
          }
        }
      ]
    }
  ],
  "flows": [
    {
      "name": "flow-1",
      "enabled": true,
      "selectors": [
        {
          "type": "http",
          "path": "/",
          "pathOperator": "START_WITH"
        }
      ]
    }
  ],
  "analytics": {
    "enabled": false
  }
}