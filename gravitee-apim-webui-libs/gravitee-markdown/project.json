{
  "name": "markdown",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "gravitee-apim-webui-libs/gravitee-markdown/src",
  "prefix": "gio",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:ng-packagr",
      "outputs": ["{workspaceRoot}/dist/@gravitee/gravitee-markdown"],
      "options": {
        "project": "gravitee-apim-webui-libs/gravitee-markdown/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "gravitee-apim-webui-libs/gravitee-markdown/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "gravitee-apim-webui-libs/gravitee-markdown/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "development"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "gravitee-apim-webui-libs/gravitee-markdown/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/gravitee-markdown"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["gravitee-apim-webui-libs/gravitee-markdown/**/*.ts"]
      }
    },
    "lint-license": {
      "executor": "nx:run-commands",
      "options": {
        "command": "license-check-and-add check -f license-check-config.json",
        "cwd": "gravitee-apim-webui-libs/gravitee-markdown"
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "configDir": "gravitee-apim-webui-libs/gravitee-markdown/.storybook",
        "browserTarget": "markdown:build",
        "styles": ["gravitee-apim-webui-libs/gravitee-markdown/.storybook/index.scss"],
        "compodoc": false,
        "port": 6007
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "options": {
        "configDir": "gravitee-apim-webui-libs/gravitee-markdown/.storybook",
        "browserTarget": "markdown:build",
        "styles": ["gravitee-apim-webui-libs/gravitee-markdown/.storybook/index.scss"],
        "compodoc": false,
        "outputDir": "gravitee-apim-webui-libs/gravitee-markdown/storybook-static"
      }
    }
  },
  "tags": ["type:lib", "scope:shared"]
}
