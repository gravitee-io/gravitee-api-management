{
    "type": "measures",
    "timeRange": {
        "from": "2025-01-01T00:00:00Z",
        "to": "2025-01-31T23:59:59Z"
    },
    "filters": [
        {
            "facet": "api",
            "operator": "eq",
            "value": "my-api"
        }
    ],
    "requests": [
        {
            "metric": "gateway_response_time",
            "filters": [
                {
                    "facet": "tenant",
                    "operator": "eq",
                    "value": "europe"
                }
            ],
            "aggregations": ["p90", "p95", "p99"]
        }
    ]
}