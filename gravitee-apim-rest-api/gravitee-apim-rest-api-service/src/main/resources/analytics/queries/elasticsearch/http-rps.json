{
  "size": 0,
  "query": {
    "bool": {
      "must": [],
      "filter": [
        {
          "range": {
            "@timestamp": {
              "gte": "now-1h"
            }
          }
        }
      ]
    }
  },
  "aggs": {
    "rps": {
      "filters": {
        "filters": {
          "all": {
            "match_all": {}
          }
        }
      },
      "aggs": {
        "count": {
          "value_count": {
            "field": "@timestamp"
          }
        },
        "time_range": {
          "stats": {
            "field": "@timestamp"
          }
        },
        "rps": {
          "bucket_script": {
            "buckets_path": {
              "count": "count",
              "min_time": "time_range.min",
              "max_time": "time_range.max"
            },
            "script": {
              "source": "params.min_time != null && params.max_time != null && (params.max_time - params.min_time) > 0 ? params.count / ((params.max_time - params.min_time) / 1000) : 0"
            }
          }
        }
      }
    }
  }
}
