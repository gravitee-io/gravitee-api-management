          "maxConcurrentConnections": 100,
          "useCompression": true,
          "followRedirects": false,
          "maxHeaderSize": 8192,
          "maxChunkSize": 8192
        }
      }
    ]
  },
  "response_templates": {
    "API_KEY_MISSING": {
      "*/*": {
        "status": 400,
        "body": "{\"bad\":\"news\"}",
        "propagateErrorKeyToLogs": false
      }
    }
  },
  "disable_membership_notifications" : false
