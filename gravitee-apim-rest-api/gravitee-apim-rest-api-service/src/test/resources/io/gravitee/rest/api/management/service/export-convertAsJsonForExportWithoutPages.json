          {
            "methods": [
              "GET"
            ],
            "rate-limit": {
              "rate": {
                "limit": 1,
                "periodTime": 1,
                "periodTimeUnit": "SECONDS"
              }
            },
            "enabled": true
          }
        ]
      },
      "flows": [],
