    "@angular/router": "19.2.4",
    "@asciidoctor/core": "3.0.4",
    "@asyncapi/web-component": "2.6.3",
    "@emoji-mart/data": "1.2.1",
    "@fontsource/dm-mono": "5.2.5",
    "@fontsource/dm-sans": "5.2.5",
