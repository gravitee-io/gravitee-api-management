  "description": "Gravitee.io APIM - performance",
  "private": true,
  "devDependencies": {
    "@types/k6": "~0.37.0",
    "@types/node": "^18.0.6",
    "@types/webpack": "5.28.0",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "license-check-and-add": "4.0.3",
    "prettier": "2.5.1",
    "ts-command-line-args": "^2.3.1",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.5.2",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-glob-entries": "^1.0.1",
