{
  "id" : "my-api",
  "name" : "my-team-api",
  "version" : "undefined",
  "gravitee" : "1.0.0",
  "flow_mode" : "DEFAULT",
  "proxy" : {
    "virtual_hosts" : [ {
      "path" : "/team"
    } ],
    "strip_context_path" : false,
    "preserve_host" : false,
    "groups" : [ {
      "name" : "default-group",
      "endpoints" : [ {
        "name" : "default",
        "target" : "http://host1:8083/myapi",
        "weight" : 1,
        "backup" : false,
        "type" : "http",
        "headers" : [
          {"name": "Host", "value": "host"}
        ]
      } ],
      "load_balancing" : {
        "type" : "ROUND_ROBIN"
      },
      "http" : {
        "connectTimeout" : 5000,
        "idleTimeout" : 60000,
        "keepAliveTimeout" : 30000,
        "keepAlive" : true,
        "readTimeout" : 10000,
        "pipelining" : false,
        "maxConcurrentConnections" : 100,
        "useCompression" : true,
        "followRedirects" : false
      }
    } ],
    "cors" : {
      "enabled" : true,
      "allowCredentials" : false,
      "allowOrigin" : [ ],
      "allowHeaders" : [ ],
      "allowMethods" : [ ],
      "exposeHeaders" : [ ],
      "maxAge" : -1
    }
  },
  "paths" : {
    "/*" : [ ]
  },
  "properties" : [ ],
  "response_templates" : {
    "API_KEY_INVALID" : {
      "application/json" : {
        "status" : 403,
        "headers" : {
          "x-header2" : "header2",
          "x-header1" : "header1"
        },
        "body" : "{}"
      },
      "text/xml" : {
        "status" : 403,
        "headers" : {
          "x-header2" : "header2",
          "x-header1" : "header1"
        },
        "body" : "<error></error>"
      },
      "*" : {
        "status" : 403,
        "headers" : {
          "x-header2" : "header2",
          "x-header1" : "header1"
        },
        "body" : "{}"
      }
    },
    "API_KEY_MISSING" : {
      "application/json" : {
        "status" : 401,
        "headers" : {
          "x-header2" : "header2",
          "x-header1" : "header1"
        },
        "body" : "{}"
      },
      "text/xml" : {
        "status" : 401,
        "headers" : {
          "x-header2" : "header2",
          "x-header1" : "header1"
        },
        "body" : "<error></error>"
      },
      "*" : {
        "status" : 401,
        "headers" : {
          "x-header2" : "header2",
          "x-header1" : "header1"
        },
        "body" : "{}"
      }
    }
  }
}
