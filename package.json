    "scripts": {
        "build": "nx run-many -t build",
        "test": "nx run-many -t test",
        "lint": "nx run-many -t lint",
        "lint:fix": "nx run-many -t lint --fix",
        "lint:eslint": "nx run-many -t lint-eslint",
        "lint:license": "nx run-many -t lint-license",
        "prettier": "nx format:check --libs-and-apps && nx format:check --files=nx.json,package.json,jest.config.js",
        "prettier:fix": "nx format:write --libs-and-apps && nx format:write --files=nx.json,package.json,jest.config.js",
        "affected:build": "nx affected -t build",
        "affected:test": "nx affected -t test",
        "affected:lint": "nx affected -t lint",
        "affected:lint:fix": "nx affected -t lint --fix",
        "affected:lint:eslint": "nx affected -t lint-eslint",
        "affected:lint:license": "nx affected -t lint-license",
        "graph": "nx graph",
        "console:serve": "NODE_OPTIONS=--max_old_space_size=7168 nx serve console",
        "console:serve:apim-master": "BACKEND_ENV=apim-master-api.team-apim.gravitee.dev NODE_OPTIONS=--max_old_space_size=7168 nx serve console",
        "console:serve:apim-gko": "BACKEND_ENV=apim-dev-api.team-gko.gravitee.dev NODE_OPTIONS=--max_old_space_size=7168 nx serve console",
        "console:build": "NODE_OPTIONS=--max_old_space_size=7168 nx build console",
        "console:build:prod": "NODE_OPTIONS=--max_old_space_size=7168 nx build console --configuration=production",
        "console:test": "nx test console",
        "console:storybook": "nx storybook console",
        "portal-next:serve": "NODE_OPTIONS=--max_old_space_size=7168 nx serve portal-next",
        "portal-next:serve:apim-master": "BACKEND_ENV=apim-master-api.team-apim.gravitee.dev NODE_OPTIONS=--max_old_space_size=7168 nx serve portal-next",
        "portal-next:build": "NODE_OPTIONS=--max_old_space_size=8192 nx build portal-next",
        "portal-next:build:prod": "NODE_OPTIONS=--max_old_space_size=8192 nx build portal-next --configuration=production",
        "portal-next:test": "nx test portal-next",
        "portal-next:storybook": "nx storybook portal-next",
        "markdown:storybook": "nx storybook markdown",
        "markdown:test": "nx test markdown",
        "dashboard:storybook": "nx storybook dashboard",
        "dashboard:test": "nx test dashboard",
