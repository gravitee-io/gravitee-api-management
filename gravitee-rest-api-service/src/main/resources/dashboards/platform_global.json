  "chart": {
    "type": "line",
    "request": {
      "type": "date_histo",
      "aggs": "avg:response-time%3Bavg:api-response-time"
    },
    "labels": ["Global latency (ms)", "API latency (ms)"]
  }
}, {
  "col": 4,
