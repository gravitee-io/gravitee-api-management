  "subhead": "Hits repartition by Host HTTP Header",
  "chart": {
    "type": "table",
    "selectable": true,
    "columns": ["Host", "Hits"],
    "paging": 5,
    "request": {
      "type": "group_by",
      "field": "host"
    }
  }
