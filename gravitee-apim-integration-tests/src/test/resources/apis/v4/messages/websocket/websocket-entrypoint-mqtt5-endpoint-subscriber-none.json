{
  "id": "my-api-none",
  "name": "my-api-none",
  "apiVersion": "1.0",
  "definitionVersion": "4.0.0",
  "type": "message",
  "description": "api v4 using Websocket entrypoint",
  "listeners": [
    {
      "type": "http",
      "paths": [
        {
          "path": "/test-none"
        }
      ],
      "entrypoints": [
        {
          "type": "websocket",
          "qos" : "none",
          "configuration": {
            "subscriber": {
              "enabled": true
            },
            "publisher": {
              "enabled": false
            }
          }
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default-group",
      "type": "mqtt5",
      "endpoints": [
        {
          "name": "default",
          "type": "mqtt5",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "serverHost": "localhost",
            "serverPort": "mqtt5-port"
          },
          "sharedConfigurationOverride": {
            "consumer": {
              "enabled": true,
              "topic": "test-topic"
            }
          }
        }
      ]
    }
  ],
  "flows": [],
  "analytics": {
    "enabled": false
  }
}
