{
  "id": "my-api",
  "name": "my-api",
  "apiVersion": "1.0",
  "definitionVersion": "4.0.0",
  "type": "message",
  "description": "API v4 using HTTP-GET entrypoint",
  "listeners": [
    {
      "type": "http",
      "paths": [
        {
          "path": "/test"
        }
      ],
      "entrypoints": [
        {
          "type": "http-get",
          "configuration": {
            "messagesLimitCount":  12,
            "messagesLimitDurationMs": 500,
            "headersInPayload":  true,
            "metadataInPayload": true
          }
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default",
      "type": "mock",
      "endpoints": [
        {
          "name": "default-endpoint",
          "type": "mock",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "messageInterval": 1,
            "messageContent": "Mock data",
            "messageCount": 15
          }
        }
      ]
    }
  ],
  "flows": [],
  "analytics": {
     "enabled" : true
  }
}
