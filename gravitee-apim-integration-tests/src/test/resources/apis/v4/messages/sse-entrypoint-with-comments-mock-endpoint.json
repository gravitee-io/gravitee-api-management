{
  "id": "my-api",
  "name": "my-api",
  "apiVersion": "1.0",
  "definitionVersion": "4.0.0",
  "type": "message",
  "description": "api v4 using SSE entrypoint",
  "listeners": [
    {
      "type": "http",
      "paths": [
        {
          "path": "/test"
        }
      ],
      "entrypoints": [
        {
          "type": "sse",
          "configuration": {
            "metadataAsComment": true,
            "headersAsComment": true
          }
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default-group",
      "type": "mock",
      "endpoints": [
        {
          "name": "default",
          "type": "mock",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "messageInterval": 1000,
            "messageContent": "{ \"message\": \"hello\" }",
            "populateHeaders": true,
            "populateMetadata": true
          }
        }
      ]
    }
  ],
  "flows": [],
  "analytics": {
    "enabled": false
  }
}
