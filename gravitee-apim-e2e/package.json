{
    "name": "gravitee-apim-e2e",
    "version": "3.16.1",
    "private": true,
    "description": "Gravitee.io APIM - e2e",
    "scripts": {
        "apim:clean": "sh ./scripts/apim.sh clean",
        "apim:serve": "sh ./scripts/apim.sh",
        "build": "tsc --removeComments --outDir dist --target es5",
        "bulk": "jest -c jest.config.bulk.js -e --forceExit --logHeapUsage --useStderr --runInBand --detectOpenHandles",
        "test:ui": "docker-compose -f ./docker/common/docker-compose-apis.yml -f ./docker/common/docker-compose-uis.yml -f ./docker/ui-tests/docker-compose-ui-tests.yml -p ui-tests --project-directory $PWD up --no-build --abort-on-container-exit --exit-code-from cypress",
        "test:ui:dev": "cypress open --config-file ui-test/integration/apim/ui/cypress-config.json",
        "test:ui:bulk": "cypress run --config-file ui-test/integration/apim/bulk/cypress-config.json",
        "test:ui:e2e": "cypress open --config-file ui-test/integration/apim/e2e/cypress-config.json",
        "test:ui:platform": "cypress open --config-file ui-test/integration/platform/cypress-config.json",
        "test:api": "docker-compose -f ./docker/common/docker-compose-apis.yml -f ./docker/api-tests/docker-compose-api-tests.yml -f ./docker/common/docker-compose-wiremock.yml -p api-integration-test --project-directory $PWD up --abort-on-container-exit --exit-code-from jest-e2e",
        "test:api:dev": "jest --runInBand --no-cache",
        "test:api:ci": "jest --runInBand --ci -c jest.config.ci.js --no-cache",
        "test:report": "ENV=ci ant -f jacoco/build.xml",
        "test:report:dev": "ENV=dev ant -f jacoco/build.xml",
        "lint": "npm run prettier && npm run lint:license",
        "lint:fix": "npm run prettier:fix && npm run lint:license:fix && tsc --noEmit",
        "lint:license": "license-check-and-add check -f license-check-config.json",
        "lint:license:fix": "license-check-and-add add -f license-check-config.json -r",
        "prettier": "prettier --check \"**/*.{js,ts,html,css,scss,json}\"",
        "prettier:fix": "prettier --write \"**/*.{js,ts,html,css,scss,json}\"",
        "prettier:list": "prettier --list-different \"**/*.{js,ts,html,css,scss,json}\"",
        "update:sdk:management": "sh ./scripts/update-management-sdk.sh",
        "update:sdk:portal": "sh ./scripts/update-portal-sdk.sh"
    },
    "license": "MIT License",
    "devDependencies": {
        "@bahmutov/cypress-extends": "^1.1.0",
        "@jest/create-cache-key-function": "^28.0.0",
        "@types/jest": "27.4.1",
        "@types/jsonwebtoken": "8.5.8",
        "@types/node": "16.10.9",
        "@types/node-fetch": "2.6.1",
        "ansi-regex": "6.0.1",
        "cypress": "9.5.0",
        "cypress-terminal-report": "3.4.1",
        "dotenv": "16.0.0",
        "har-validator": "5.1.5",
        "jest": "27.5.1",
        "license-check-and-add": "4.0.3",
        "node-fetch": "2.6.7",
        "prettier": "2.5.1",
        "ts-jest": "^27.1.5",
        "typescript": "4.5.2"
    },
    "dependencies": {
        "@faker-js/faker": "6.2.0",
        "jsonwebtoken": "8.5.1"
    }
}
